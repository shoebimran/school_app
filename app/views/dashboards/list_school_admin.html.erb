<h3 class="m-2">
  List of school admins
</h3>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <% @list_school_admins.each do |user| %>
    <tr>
      <th scope="row"><%= user&.id %></th> 
      <th><%= user&.first_name %></th>
      <td><%= user&.last_name %></td>
      <td><%= user&.email %></td>
      <td><%= link_to "Edit user",edit_dashboard_path({id:user}),class: "btn btn-primary mt-2 ml-2" %></td>
      <td><%= link_to "Delete user",dashboard_path({id: user}),data: { turbo_method: :delete,turbo_confirm: "Are you sure?"},class: "btn btn-danger mt-2 ml-2" %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<div class="display-1"></div>
<div class="display-1"></div>
<h6><%= link_to "New School Admin", new_dashboard_path, class: "btn btn-primary mt-2 ml-2" %></h6>
